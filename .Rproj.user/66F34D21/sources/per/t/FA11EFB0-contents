# Generació d'1 plot SLP

# Generació de 4 plots SLP sota una mateixa llegenda
par(mfrow = c(2,2))
  for (ii in 1:10) {
    plot_clas(mslp$lon, mslp$lat,grouped_data = clasi$grouped_data,cwt_number = ii, legend.lab = "hPa")
    title(paste("CWT ", ii))
  }
